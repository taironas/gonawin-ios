    pod 'Moya/RxSwift', :git => 'https://github.com/Moya/Moya', :tag => '8.0.0-beta.1'
    pod 'RxSwift', :git => 'https://github.com/ReactiveX/RxSwift', :tag => '3.0.0-beta.1'
    pod 'SwiftyJSON', :git => 'https://github.com/IBM-Swift/SwiftyJSON.git'
    pod 'GonawinEngine', :git => 'https://github.com/taironas/gonawin-engine-ios.git'
    pod 'FontAwesomeKit/FontAwesome'
    pod 'Locksmith', :git => 'https://github.com/matthewpalmer/Locksmith', :branch => 'swift-3.0'
    pod 'SwiftyUserDefaults'
    pod 'p2.OAuth2', :git => 'https://github.com/p2/OAuth2', :submodules => true
    pod 'LVTwitterOAuthClient'
end
